#include <iostream>

using namespace std;

int main()
    {
    for(char i = 1; i <= 100; ++i)     //Each number starting at 1 and ending at 100.
        {
        if(i%3==0)
	       cout << "Fizz";             // If i is a multiple of 3, display Fizz
    	if(i%5==0)                     // If i is a multiple of 5, display Buzz
    	   cout << "Buzz";             // If i is a multiple of both, FizzBuzz will be displayed
    	                               
    	else if(i%3 != 0)              // If i is a multiple of neither, display i
       	   cout << (int)i;
	    cout << endl;
     	}
    }
